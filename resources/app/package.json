{
  "name": "trade-butler-gui",
  "productName": "Trade Butler Bot",
  "version": "2.5.5",
  "description": "A local limit trading bot intended for use with DEXs",
  "main": "main.js",
  "bin": "cli.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug",
    "test": "mocha && standard",
    "generate-test-report": "mocha --reporter=json > report.json",
    "package": "npm-run-all package:*",
    "package:mac": "electron-packager . --platform=darwin --arch=x64 --app-bundle-id='com.draukan.tradebutler' --app-version='2.5.5' --build-version='2.5.500' --out=release-builds --overwrite --icon=assets/app-icon/mac/app.icns",
    "package:win": "electron-packager . --platform=win32 --arch=x64 --app-bundle-id='com.draukan.tradebutler' --app-version='2.5.5' --build-version='2.5.500' --out=release-builds --overwrite --prune=true --icon=assets/app-icon/win/app.ico",
    "package:linux": "electron-packager . --platform=linux --arch=x64 --app-bundle-id='com.draukan.tradebutler' --app-version='2.5.5' --build-version='2.5.500' --out=release-builds --overwrite --prune=true --icon=assets/app-icon/png/1024.png",
    "package:arm": "electron-packager . --platform=linux --arch=armv7l --app-bundle-id='com.draukan.tradebutler' --app-version='2.5.5' --build-version='2.5.500' --out=release-builds --overwrite --prune=true --icon=assets/app-icon/png/1024.png",
    "package:sign-exe": "signcode './out/TBB-win32-ia32/TBB-GUI.exe' --cert ~/electron-api-demos.p12 --prompt --name 'Trade Butler Bot' --url 'https://tradebutlerbot.app'",
    "package:installer": "node ./script/installer.js",
    "package:sign-installer": "signcode './out/windows-installer/TradeButlerBot.exe' --cert ~/electron-api-demos.p12 --prompt --name 'Trade Butler Bot' --url 'https://tradebutlerbot.app'",
    "xpackage:mas": "./script/mas.sh",
    "windows-store": "node ./script/windows-store.js",
    "release": "node ./script/release.js",
    "prepack": "check-for-leaks",
    "prepush": "check-for-leaks",
    "upload": "curl -s -u draukan -X POST https://api.bitbucket.org/2.0/repositories/draukan/trade-butler-bot-release/downloads -F files=@Trade-Butler-Bot-win32-x64-v2.5.4-unaudited.zip"
  },
  "repository": "https://github.com/electron/electron-api-demos",
  "keywords": [
    "Trade Butler",
    "bot",
    "uniswap"
  ],
  "author": "draukan",
  "license": "MIT",
  "devDependencies": {
    "@octokit/rest": "^16.3.2",
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "check-for-leaks": "^1.2.1",
    "devtron": "^1.3.0",
    "electron-packager": "^12.2.0",
    "electron-winstaller": "^2.7.0",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.0.2",
    "request": "^2.70.0",
    "rimraf": "^2.5.2",
    "signcode": "^0.5.0",
    "spectron": "^5.0.0",
    "standard": "^8.2.0",
    "tap": "^14.10.6"
  },
  "dependencies": {
    "@pancakeswap-libs/sdk": "^1.0.1",
    "@pancakeswap-libs/sdk-v2": "^2.1.0",
    "@spectrum-css/button": "^3.0.0-beta.6",
    "@spectrum-css/page": "^3.0.0-beta.0",
    "@spectrum-css/vars": "^3.0.0-beta.2",
    "@sushiswap/sdk": "^3.0.1",
    "@thugs-defi/sdk": "0.0.2",
    "@uniswap/sdk": "^3.0.3",
    "bigdecimal": "^0.6.1",
    "bytenode": "^1.1.7",
    "dexie": "^3.0.3",
    "electron": "^7.2.4",
    "electron-json-storage": "^4.3.0",
    "electron-log": "^2.2.14",
    "electron-redux": "^1.5.4",
    "electron-settings": "^3.0.7",
    "electron-shortcut-normalizer": "^1.0.0",
    "electron-store": "^6.0.1",
    "ethers": "^5.0.24",
    "glob": "^7.1.0",
    "jquery": "^3.5.1",
    "node-fetch": "^2.6.1",
    "update-electron-app": "^2.0.1",
    "web3": "^1.3.1"
  },
  "standard": {
    "env": {
      "mocha": true
    }
  }
}
